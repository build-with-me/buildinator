{
  "imports": {
    "std/": "https://deno.land/std@0.185.0/",
    "oak/": "https://deno.land/x/oak@v12.4.0/"
  },
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "lint": {
    "include": ["src/"],
    "exclude": [],
    "rules": {
      "tags": ["recommended"],
      "include": ["ban-untagged-todo"],
      "exclude": ["no-unused-vars"]
    }
  },
  "fmt": {
    "useTabs": true,
    "lineWidth": 80,
    "indentWidth": 4,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": ["src/"],
    "exclude": []
  }
}
